        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.betagro.adulvitc.drugdosecalculator2.MainActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.betagro.adulvitc.drugdosecalculator2.DetailActivity" />
        <activity android:name="com.betagro.adulvitc.drugdosecalculator2.CalculateActivity" />
        <activity android:name="com.betagro.adulvitc.drugdosecalculator2.ResultActivity" />
        <activity android:name="com.betagro.adulvitc.drugdosecalculator2.RegisterActivity" />
        <activity android:name="com.betagro.adulvitc.drugdosecalculator2.UserAreaActivity" />
        <activity android:name="com.betagro.adulvitc.drugdosecalculator2.Login2Activity" />
        <activity android:name="com.betagro.adulvitc.drugdosecalculator2.MyProfileActivity" />
        <activity android:name="com.betagro.adulvitc.drugdosecalculator2.VitaminActivity"></activity>
    </application>

